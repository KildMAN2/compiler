// Test float comparisons
void main() {
    x:float;
    y:float;
    
    x = 3.14;
    y = 2.71;
    
    if x > y then {
        write("3.14 > 2.71\n");
    }
    
    if x >= y then {
        write("3.14 >= 2.71\n");
    }
    
    if y < x then {
        write("2.71 < 3.14\n");
    }
    
    if y <= x then {
        write("2.71 <= 3.14\n");
    }
}
