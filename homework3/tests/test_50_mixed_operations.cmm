// Test mixed arithmetic, logical, and control flow
int calculate(x:int, y:int) {
    result:int;
    
    if ((x > 0) and (y > 0)) then {
        result = x + y;
    } else {
        if ((x < 0) and (y < 0)) then {
            result = x * y;
        } else {
            result = 0;
        }
    }
    
    return result;
}

void main() {
    neg5:int;
    neg10:int;
    
    neg5 = 0 - 5;
    neg10 = 0 - 10;
    
    write(calculate(x:5, y:10));
    write(calculate(x:neg5, y:neg10));
    write(calculate(x:5, y:neg10));
}
