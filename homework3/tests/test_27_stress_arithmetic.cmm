// Stress test with heavy arithmetic
void main() {
    i:int;
    sum:int;
    product:int;
    
    i = 1;
    sum = 0;
    product = 1;
    
    while i <= 20 do {
        sum = sum + i;
        if i <= 10 then {
            product = product * i;
        }
        i = i + 1;
    }
    
    write("Sum: ");
    write(sum);
    write("\n");
    write("Product: ");
    write(product);
    write("\n");
}
