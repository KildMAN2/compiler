// Test logical OR
void main() {
    a:int;
    b:int;
    
    a = 5;
    b = 10;
    
    if a > 10 or b > 5 then {
        write("At least one true\n");
    } else {
        write("Both false\n");
    }
    
    if a < 0 or b < 0 then {
        write("At least one negative\n");
    } else {
        write("Both non-negative\n");
    }
}
