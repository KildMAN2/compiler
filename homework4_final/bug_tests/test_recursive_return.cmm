# Test: Recursive function with return value
# Expected: Should work correctly and return proper values
# Bug: Problems in return value of recursive functions

int factorial(n:int) {
    if n <= 1 then 
        return 1;
    else
        return n * factorial(n - 1);
}

void main() {
    write("5! = ");
    write(factorial(5));
    write("\n");
}
